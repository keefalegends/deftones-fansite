{
  "family": "deftones-fansite",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "taskRoleArn": "arn:aws:iam::101606666695:role/LabRole",
  "executionRoleArn": "arn:aws:iam::101606666695:role/LabRole",
  "containerDefinitions": [
    {
      "name": "deftones-fansite",
      "image": "nginx:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/deftones-fansite",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}